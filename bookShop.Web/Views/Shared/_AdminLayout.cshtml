                @RenderBody()
            </div>

        </div>

    </div>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="~/AdminTema/assets/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/AdminTema/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="~/AdminTema/assets/js/off-canvas.js"></script>
    <script src="~/AdminTema/assets/js/hoverable-collapse.js"></script>
    <script src="~/AdminTema/assets/js/misc.js"></script>
    <script src="assets/js/dashboard.js"></script>
    <script src="assets/js/todolist.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
